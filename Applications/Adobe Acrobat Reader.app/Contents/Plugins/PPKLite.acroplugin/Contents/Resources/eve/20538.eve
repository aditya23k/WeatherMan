dialog(name: '$$$/Dialogs/ExportSecSettings')
{
	group( name: '$$$/Dialogs/ExportSecSettings/SettingsToExport', horizontal: align_fill )
	{
		checkbox( identifier: 'AuUS', name: '$$$/Dialogs/ExportSecSettings/AutomaticUpdate' );
		checkbox( identifier: 'TruS', name: '$$$/Dialogs/ExportSecSettings/TrustSettings' );
		checkbox( identifier: 'SigP', name: '$$$/Dialogs/ExportSecSettings/SigningPreferences' );
		checkbox( identifier: 'SigV', name: '$$$/Dialogs/ExportSecSettings/SignatureValidation' );
		checkbox( identifier: 'SecP', name: '$$$/Dialogs/ExportSecSettings/SecurityPolicies' );
		checkbox( identifier: 'DgID', name: '$$$/Dialogs/ExportSecSettings/DigitalIDs' );
		checkbox( identifier: 'RmID', name: '$$$/Dialogs/ExportSecSettings/RoamingIDs' );
		checkbox( identifier: 'SerC', name: '$$$/Dialogs/ExportSecSettings/ServerConnections' );
		row( horizontal: align_fill )
		{
			button( identifier: 'sAll', name: '$$$/Dialogs/ExportSecSettings/SelectAll' );
			button( identifier: 'dAll', name: '$$$/Dialogs/ExportSecSettings/DeselectAll' );
		}
	}
	ok_cancel();
}
